      "required": true,
      "entityType": "@sys.any",
      "fillBehavior": {
        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "What is the first and last name of the passenger flying?" ]
            },
            "languageCode": "en"
          } ]
        }
      }
    }, {
      "displayName": "departure_date",
      "required": true,
      "entityType": "@sys.date",
