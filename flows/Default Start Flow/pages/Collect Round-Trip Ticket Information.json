        "initialPromptFulfillment": {
          "messages": [ {
            "text": {
              "text": [ "What time would you like to leave on the outbound flight? Please use AM and PM in your response." ]
            },
            "languageCode": "en"
          } ]
        }
      }
    }, {
      "displayName": "return_date",
      "required": true,
      "entityType": "@sys.date",
